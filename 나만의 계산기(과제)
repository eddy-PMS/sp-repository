fun main() {
    var num1 = readLine()!!.toInt()
    var operator = readLine()
    var num2 = readLine()!!.toInt()

    var add = num1 + num2
    var subtract = num1 - num2
    var multiply = num1 * num2
    var divide = num1 / num2



    when(operator) {
        "+" -> { println("${add}" ) }
        "-" -> { println("${subtract}" ) }
        "*" -> { println("${multiply}") }
        "/" -> { println("${divide}") }


    }

}
